{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "7.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.4.3"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "platform": {
      "php": "7.3",
      "ext-something": "1"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/phpunit/phpunit/phpunit",
    "auto-scripts": {
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  }
}
